<div class="container">
  <div class="row col-6 mt-3 mb-5">
    <%= simple_form_for(@survey) do |f| %>
      <div data-toggle-target="errors">
        <%= f.error_notification %>
      </div>
      <%= f.input :title %>
      <%= f.input :interval_days %>
      <%= f.submit :Create, class: 'btn btn-primary my-custom-class' %>
    <% end %>
  </div>
</div>
